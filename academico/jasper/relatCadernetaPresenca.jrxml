   ALCRG,
   ALCOERG,
   ALCUFRG,
   ALCTIEL,
   ALCZOTE,
   ALCSETE,
   ALDDETE,
   ALNCGRL,
   ALCNACI,
   ALCIESG,
   ALCFONE,
   ALCCELL,
   ALNCGTL,
   ALCLOGR,
   ALNCDED,
   ALCCOMP,
   ALCCEP,
   ALCBAIR,
   ALDCADT,
   ALLATIV,
   PFNCODG,
   PFCNOME,
   PFLATIV,
   UENCODG,
   UECDESC,
   UELATIV,
   CMNCODG,
   CMNCGSM,
   CMNCGCS,
   CMNCGUE,
   CMNCGTN,
   CMYVALR,
   CMYMULT,
   CMYJURO,
   CMYDESC,
   CMNMDIA,
   CMLATIV,
   SMNCODG,
   SMNCGAN,
   SMNNUMR,
   SMCDESC,
   SMDABER,
   SMDFECH,
   SMLATIV,
   ANNCODG,
   ANNANO,
   TNNCODG,
   TNCDESC,
   CSNCODG,
   CSCDESC,
   CSCTIPO,
   CSNMDIA,
   CSCSIGL,
   CSLATIV
 FROM
   MATRICULATURMA LEFT JOIN TURMA ON
   TURMA.TMNCODG = MATRICULATURMA.TTNCGTM
   LEFT JOIN  CURSOSEMESTRE ON
   TURMA.TMNCGCM = CURSOSEMESTRE.CMNCODG
   LEFT JOIN CURSO ON
   CURSOSEMESTRE.CMNCGCS = CURSO.CSNCODG
   LEFT JOIN TURNO ON
   CURSOSEMESTRE.CMNCGTN = TURNO.TNNCODG
   LEFT JOIN SEMESTRE ON
   CURSOSEMESTRE.CMNCGSM = SEMESTRE.SMNCODG
   LEFT JOIN ANOLETIVO ON
   SEMESTRE.SMNCGAN = ANOLETIVO.ANNCODG
   LEFT JOIN PROFESSOR ON
   TURMA.TMNCGPF = PROFESSOR.PFNCODG
   LEFT JOIN DISCIPLINA ON
   TURMA.TMNCGDC = DISCIPLINA.DCNCODG
   LEFT JOIN MATRICULA ON
   MATRICULATURMA.TTNCGMT = MATRICULA.MTNCODG
   LEFT JOIN ALUNO ON
   MATRICULA.MTNCGAL = ALUNO.ALNCODG
   LEFT JOIN UNIDADEENSINO ON
   MATRICULA.MTNCGUE = UNIDADEENSINO.UENCODG
where
  TMNCODG = $P{P_TMNCODG}
   ORDER BY ALCNOME]]></queryString>

