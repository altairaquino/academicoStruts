	<queryString><![CDATA[SELECT
   ANNANO,
   SMNNUMR,
   ALNCODG,
   ALCNOME,
   ALCCPF,
   ALCSEXO,
   ALDNASC,
   ALNCDNC,
   ALBFOTO,
   ALCPAI,
   ALCMAE,
   ALNCGEC,
   ALCRG,
   ALCOERG,
   ALCUFRG,
   ALCTIEL,
   ALCZOTE,
   ALCSETE,
   ALDDETE,
   ALNCGRL,
   ALCNACI,
   ALCIESG,
   ALCFONE,
   ALCCELL,
   ALNCGTL,
   ALCLOGR,
   ALNCDED,
   ALCCOMP,
   ALCCEP,
   ALCBAIR,
   ALDCADT,
   UENCODG,
   UECDESC,
   CSNCODG,
   CSCDESC,
   DCNCODG,
   DCCDESC,
   TMNCODG,
   TMNPERI,
   STNCODG,
   STCDESC,
   STCSIGL,
   DCNCGHR,
   DCNCRED
 FROM
   MATRICULATURMA
   left join MATRICULA on
   TTNCGMT = MTNCODG
   left join TURMA on
   ttncgtm = tmncodg
   left join aluno on
   mtncgal = alncodg
   LEFT JOIN DISCIPLINA ON
   DCNCODG = TMNCGDC
   LEFT JOIN CURSOSEMESTRE ON
   TMNCGCM = CMNCODG
   LEFT JOIN SEMESTRE ON
   CMNCGSM = SMNCODG
   LEFT JOIN ANOLETIVO ON
   SMNCGAN = ANNCODG
   LEFT JOIN CURSO ON
   CMNCGCS = CSNCODG
   LEFT JOIN UNIDADEENSINO ON
   CMNCGUE = UENCODG
   LEFT JOIN SITUACAO ON
   TTNCGST = STNCODG
WHERE
 ALNCODG = $P{ALUNO} AND
  STLHIST = 'T'
  ORDER BY
   ANNANO,
   SMNNUMR,
   TMNPERI,
   DCNCODG]]></queryString>

